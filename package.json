  "license": "MIT",
  "devDependencies": {
    "bower": "*",
    "shelljs": "*",
    "del": "*",

    "http-server": "*",
    "connect": "*",
    "connect-livereload": "*",

    "karma": "*",
    "protractor": "*",
    "karma-junit-reporter": "*",

    "gulp": "*",
    "gulp-order": "*",
    "gulp-filter": "*",
    "gulp-insert": "*",
    "gulp-concat": "*",
    "gulp-csslint": "*",
    "gulp-jshint": "*",
    "gulp-add-src": "*",
    "gulp-traceur": "*",
    "gulp-livereload": "*",
    "gulp-sourcemaps": "*",
    "main-bower-files": "*",
    "gulp-load-plugins": "*",
    "jshint-stylish": "*",
    "opn": "*"
  },
  "scripts": {
    "postinstall": "bower install",

    "prestart": "npm install",
    "start": "http-server -a localhost -p 8000 -c-1",

    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js  --single-run",

